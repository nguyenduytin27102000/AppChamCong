@model IEnumerable<QuanLyChamCong_Demo.Models.ChinhSachTheoThamNien>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ThamNien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoNgayTang)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Xoa)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ThamNien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoNgayTang)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Xoa)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaThamNien">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaThamNien">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaThamNien">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
